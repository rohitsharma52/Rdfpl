                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                        <% }) %>
                      </div>
                      

                    <!-- Swiper Navigation -->
                    <div class="wptb-swiper-navigation style1">                      
                        <div class="wptb-swiper-arrow swiper-button-prev"></div>
                        <div class="wptb-swiper-arrow swiper-button-next"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- //////////////////////////////////////////////OUR BLOG////////////////////////////////////////////////////////////////////                       -->
<section class=" blog_bg wptb-slider style9">
    <div style="padding-top: 4rem;">
        <h1 class="wptb-item--title  wow animate__bounceInDown text-center">OUR BLOGS</h1>
    </div>

    <div class="swiper-container wptb-swiper-slider-nine" id="blog"> 
        <!-- swiper slides -->
        <div class="swiper-wrapper">
            <!-- Slide Item -->
            <% blog_data.forEach(function(b_data){        %>  
            <div class="swiper-slide">
                <div  class="wptb-slider--item">
                    <div class="wptb-slider--inner">
                        <img src="<%= b_data.image %>" >
                        <div class="trans_bg wptb-heading">
                            <h1 class="wptb-item--title mt-2"><%=b_data.heading%></h1>
                    
                            <h6 class="wptb-item--subtitle mt-2">BY <%=b_data.by_blog%></h6>
                            <h6 class="wptb-item--subtitle mt-3 mb-4"><%= b_data.formattedDate%></h6>
                            
                        </div>
                    </div>

                    <a class="wptb-image-popup" href="<%= b_data.image %>" data-fancybox="gallery">
                        <i class="bi bi-arrows-fullscreen"></i>
                    </a>
                </div>
            </div>
            <%})%>
        </div>
    </div>

   

   

    <!-- Bottom Pane -->
    <div class="wptb-bottom-pane justify-content-center">
        <!-- pagination dots -->
        <div class="wptb-swiper-dots style2">
            <div class="swiper-pagination"></div>
        </div>

        <!-- Swiper Navigation -->
        <div class="wptb-swiper-navigation style3">                      
            <div class="wptb-swiper-arrow swiper-button-prev"></div>
            <div class="wptb-swiper-arrow swiper-button-next"></div>
        </div>
    </div>
    
</section>
<!-- /////////////////////////////////// Our Store///////////////////////////////////////////////////////// -->

    <section class=" store_bg  wptb-slider style5" >
        <div>
            <h1 class=" mt-5 wptb-item--title  wow animate__bounceInDown text-center">OUR STORES</h1>
        </div>
        <div class="swiper-container wptb-swiper-slider-five"id="our-store">    
            <!-- swiper slides -->
            <div class="swiper-wrapper">
                <!-- Slide Item -->
                <% Store_data.forEach(function(s_data){        %>       
        
                <div class="swiper-slide">
                    <div class="wptb-slider--item">
                        
                        <div class=" store_img">
                            <div class="  heading_style text-center d-flex">
                                <h1 class="wptb-item--title ms-2"><%=s_data.Heading%></h1>
                                <h3  class="mt-2 ms-2">[<%=s_data.title%>]</h3>
                            </div>
                            <a href="blog-details.html" class="wptb-item-link"><img src="<%=s_data.image%>"  alt="img"></a>
                        </div>    
                       
                    </div>
                </div>
                <%})%>
                <!-- End Slide Item -->
    
                <!-- Slide Item -->
                
               
            </div>
        </div>
    
        <!-- Swiper Navigation -->
        <div class="wptb-swiper-navigation style2 d-none d-lg-flex">                      
            <div class="wptb-swiper-arrow swiper-button-prev"></div>
            <div class="wptb-swiper-arrow swiper-button-next"></div>
        </div>
    
        <!-- Bottom Pane -->
        <div class="wptb-bottom-pane justify-content-center d-lg-none">
            <!-- pagination dots -->
            <div class="wptb-swiper-dots style2">
                <div class="swiper-pagination"></div>
            </div>
        
            <!-- Swiper Navigation -->
            <div class="wptb-swiper-navigation style3">                      
                <div class="wptb-swiper-arrow swiper-button-prev"></div>
                <div class="wptb-swiper-arrow swiper-button-next"></div>
            </div>
        </div>
    
    </section>
 

</main>

<%- include('../front/common/footer') %>
