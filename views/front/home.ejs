
<%- include('../front/common/header') %>
<main class="wrapper">
    <section class=" wptb-slider style13">
        <div class="swiper-container wptb-swiper-slider-thirteen">    
            <!-- swiper slides -->
            <div class="swiper-wrapper">
                <!-- Slide Item -->
               <%  Banner_data.forEach(function(ban_data){    %>  
                <div class="swiper-slide">
                    <div class="wptb-slider--item">
                        <div class="wptb-slider--image">
                            
                            <img src="<%=ban_data.image%>" alt="">
                        </div>
                    </div>
                </div>
                <%}) %>
               
            </div>
        </div>

        <!-- Bottom Pane -->
        <div class="wptb-bottom-pane justify-content-center">

            <!-- Swiper Navigation -->
            <div class="wptb-swiper-navigation style1">                      
                <div class="wptb-swiper-arrow swiper-button-prev"></div>
                <div class="wptb-swiper-arrow swiper-button-next"></div>
            </div>
        </div>
    </section>
    <!-- //////////////////////////////////////////////Company name with silder////////////////////////////////////////////////////////////////////                       -->
  
     <!-- ////////////////////////////////our brand ///////////////////////////////////////// -->
     <section class="brand py-5">
        <div class="container-fluid">
          <div class="row">
            <div class="wptb-heading col-12">
              <div class="wptb-item--inner">
                <h1 class="bounce mb-0 wow animate__backInLeft">OUR BRANDS</h1>
              </div>
            </div>
            <div class="col-6 col-md-3">
              <img src="/front/assets/png/Royal_Dryfruit_1955-Logo.png" class="wow animate__bounceInUp">
            </div>
            <div class="col-6 col-md-3">
              <img src="/front/assets/png/4Health_Logo.png" class="wow animate__bounceInLeft">
            </div>
            <div class="col-6 col-md-3">
              <img src="/front/assets/png/11Eleven_Logo.png" class="wow animate__bounceInDown">
            </div>
            <div class="col-6 col-md-3">
              <img src="/front/assets/png/Goa_Kajuwalla_Logo_PNG.png" class="wow animate__bounceInRight">
            </div>
          </div>
        </div>
      </section>
      
  
    
<!-- //////////////////////////////////////////////owner team with overlaping////////////////////////////////////////////////////////////////////                       -->
<section class="  pt-0"id="owner">
    <div class="container-fluid">
        <div class="wptb-heading">
            <div class="wptb-item--inner">
                <div class="row align-items-center">
                    <div class="col-lg-7">
                        <h1 class="wptb-heading bounce wow animate__bounceInDown"> COMPANY MD</h1>
                    </div>
                    
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                <div class="wptb-image-single wow fadeInUp">
                    <div class="wptb-item--inner">
                        <div class="wptb-item--image position-relative">
                            <img src="/front/assets/img/more/7.png" alt="img">
                            
                           
                        </div>
                    </div>
        
                    
                </div>
            </div>

            <div class="col-md-6 ps-md-5 mt-4 mt-md-0">
                <div class="wptb-about--text ps-md-5">
                    <h3>About C.E.O</h3>
                    <p class="wptb-about--text-one">Kimono photography Agency runs wide and deep. Across many markets, geographies & typologies, our team members</p>
                    <p>The talent at kimono runs wide range of services. Across many markets, geographies & typologies, our team members are some of the finest people of  photographers in the industry wide and deep. From Across many markets, geographies & boundaries. Hire Kimono in your event.</p>
                </div>
            </div>
        </div>
    </div>
</section>
       
<!-- //////////////////////////////////////////////our team////////////////////////////////////////////////////////////////////                       -->

   
 <!-- //////////////////////////////////////////////our best category////////////////////////////////////////////////////////////////////                       -->
 <section class=" category_bg wptb-slider style15">
  
        
            <h1 class="wptb-item--title bounce wow animate__bounceInDown text-center">BEST CATEGORY</h1>
        
    <div class="swiper-container wptb-swiper-slider-fifteen"id="gift-item">    
        
        <!-- swiper slides -->
        <div class="swiper-wrapper">
            <!-- Slide Item --> 
            <% cate_data.forEach(function(c_data) { %> 
            <div class="swiper-slide">
                <div class="wptb-slider--item">
                    <div class="">
                        <img src="<%=c_data.image%>" alt="">
                        <h4><a href="project-details.html"><%=c_data.name%></a></h4>

                    </div>
                    
                </div>
            </div>
         <%}) %>
        </div>
    </div>

   
    
</section>  
 
<!-- //////////////////////////////////////////////What we offer////////////////////////////////////////////////////////////////////                       -->
<section class=" offer_bg wptb-slider style15">
    <div class="wptb-heading-two text-center">
            <h1 class="wptb-item--title bounce wow animate__bounceInDown"> WHAT WE ARE <br> <span>OFFER</span> YOU</h1>
    </div>

    <div class="swiper-container wptb-swiper-slider-fifteen"id="gift-item">    
        <!-- swiper slides -->
        <div class="swiper-wrapper">
            <!-- Slide Item --> 
        <% gift_data.forEach(function(g_data){    %>     
            <div class="swiper-slide">
                <div class="wptb-slider--item">
                    <div class="slider_image">
                        <img src="<%=g_data.image%>" alt="">
                    </div>
                </div>
            </div>
         <%}) %>
        </div>
    </div>

   
    
</section>  
<!-- //////////////////////////////////////////////TESTIMONIAL////////////////////////////////////////////////////////////////////                       -->
