                                    <div class="wptb-item--image">
                                      <img src="<%= t_data.image %>" alt="img">
                                    </div>
                                    <div class=" wptb-item--meta-left">
                                      <h4 class="wptb-item--title"><%= t_data.name %></h4>
                                      <h6 class="wptb-item--designation"><%= t_data.place %></h6>
